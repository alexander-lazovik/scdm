<div class="center-role-form col-12 form " >
  <%= form_for( @login_user, url: login_path ) do |f| %>
  <div class="sign-in">
    Sign In
  </div>
  <row>
    <div class="col-md-6 col-md-offset-3" >
      <div class="form-group">
        <%= f.label :email_address, "Email Address" %>
        <div class="input-group" >
          <span class="input-group-addon"><span class="glyphicon glyphicon-user"></span></span>
          <%= f.email_field(:email_address, class: "form-control", required: true) %>
        </div>
      </div>

      <div class="form-group">
        <%= f.label :password %>
        <div class="input-group" >
          <span class="input-group-addon"><span class="glyphicon glyphicon-lock"></span></span>
          <%= f.password_field(:password, class: "form-control", required: true) %>
        </div>
      </div>

      <%=f.submit 'Sign In', class: "btn btn-primary btn-block btn-lg ", style: "height:auto" %>
      <br>
      <div class="col-sm-4 text-center"><%= link_to "Forgot Password", new_forgot_password_path %></div>
      <div class="col-sm-4 text-center"><%= link_to 'Contact Us', miscpage_path(id: 'contactus'), page: 'contactus'  %></div>
      <div class="col-sm-4 text-center"><%= link_to 'FAQ', miscpage_path(id: 'faq'), page: 'faq' %></div>
    </div>
  </row>
  <p></p>
  <% end %>
</div>
